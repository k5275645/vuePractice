	var vm1 = new Vue({
		// Vue 객체가 관리할 요소들이 만들어지기 전에 호출되는 함수
		beforeCreate : function() {
			console.log('Vue객체가 관리할 요소들이 만들어지기 전입니다.');
		},
		// Vue 객체가 관리할 요소들이 만들어진 이후
	
	})
</script>
